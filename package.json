{
  "name": "ss-webpack",
  "version": "1.0.0",
  "description": "A silverstripe theme setup using webpack to bundle assets and provide a front end build system. ",
  "main": "index.js",
  "scripts": {
    "postinstall": "node postInstall.js && npm run build",
    "watch": "NODE_ENV=development webpack-dashboard -- webpack-dev-server",
    "dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack -p --progress"
  },
  "repository": "https://github.com/AndrewHaine/silverstripe-webpack-theme",
  "author": "Andrew Haine",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.6.0",
    "import-glob-loader": "^1.1.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.1",
    "normalize.css": "^7.0.0",
    "postcss": "^5.2.18",
    "postcss-loader": "^2.1.5",
    "postcss-reporter": "^4.0.0",
    "postcss-sass": "^0.3.2",
    "postcss-scss": "^1.0.6",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.12.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-notifier": "^1.6.0"
  }
